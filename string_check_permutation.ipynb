   "source": [
    "# Determine if a string is a permutation of another string.\n",
    "\n",
    "def is_permutation(str1, str2):\n",
    "    if sorted(str1)==sorted(str2):\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "\n",
    "is_permutation(\"cat\", \"tac\")"
   ]
  }
